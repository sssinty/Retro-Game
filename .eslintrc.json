{
  "extends": "airbnb-base",
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "rules": {
    "linebreak-style": "off",
    "max-len": "off",
    "no-param-reassign":"off",
    "no-loop-func":"off",
    "guard-for-in":"off",
    "consistent-return":"off",
    "no-new":"off",
    "eol-last":"off",
    "no-alert":"off",
      "no-restricted-syntax": [
          "error",
          "LabeledStatement",
          "WithStatement"
      ]
 },
 "parserOptions": {
  "ecmaVersion": 2018,
  "sourceType": "module"
 }
}